{
  "name": "ImagingUAV Dev",
  "build": {
    "dockerfile": "../Dockerfile", 
    "context": ".."
  },
  "workspaceFolder": "/app",
  "settings": {
    "python.defaultInterpreterPath": "/usr/local/bin/python"
  },
  "extensions": [
    "ms-python.python",
    "ms-python.vscode-pylance"
  ],
  "postCreateCommand": "pip install -r requirements.txt || true"
}
